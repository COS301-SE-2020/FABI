{
    "psi-header.config": {
    "license": "Custom",
    "blankLinesAfter": 4,
    "forceToTop": true
},
"psi-header.variables": [
    [
        "company",
        "Runtime Terrors"
    ],
    [
        "projectname",
        "FABI-Surveillance"
    ],
    [
        "author",
        "Shaun Naude"
    ],
    [
        "authormail",
        "shaunnaude20@gmail.com"
    ],
    [
        "website",
        "https://shaunnaude.github.io/"
    ],
    [
        "initials",
        "SN"
    ]
],
"psi-header.templates": [
    {
        "language": "*",
        "template": [
            "File Name: <<filename>>",
            "",
            "The University of Pretoria",
            "Computer Science Department",
            "<<projectname>>",
            "<<company>>",
            "",
            "(c) Copyright 2020 University of Pretoria",
            "ALL RIGHTS RESERVED",
            "",
            "Input                          : <Inputs if any, otherwise state None>",
            "Output                         : <Outputs if any, otherwise state None>",
            "Related Requirements           : <Requirements supported (R1. Register)>",
            "Classes in this file           : <List any classes contained>",
            "Related Documents              : <Name of documents, URL if possible>",
            "",
            "Notes:",
            "           Version:            :<<projectversion>>",
            "           Author:             :<<author>> - <<authormail>>",
            "           Creation Date:      :<<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
            "HISTORY:",
            "Date       \t          By\tComments",
            "-----------\t          ---\t-----------------------------------------------------",
            "",
            "Functional Description         : <Provide a brief explanation of the file's purpose>",
            "Constraints                    : <Explain constraints if any, otherwise state None>",
            "Assumptions                    : <Explain assumptions if any, otherwise state None>"
        ],
        "changeLogCaption": "HISTORY:",
        "changeLogHeaderLineCount": 2,
        "changeLogEntryTemplate": [
            "<<dateformat('YYYY-MM-DD-hh-mm-a')>>\t  <<initials>>\t"
        ]
    }
]
}